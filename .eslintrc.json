{
  "rules": {
    "no-console": 0,
    "no-trailing-spaces": 2,
    "eqeqeq": 2,
    "no-shadow": 2,
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "never"
      }
    ],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "jasmine": true,
    "protractor": true
  },
  "globals": {
    "takeScreenshot": false,
    "login": false
  },
  "extends": "eslint:recommended"
}
